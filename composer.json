{
    "name": "nodew/dfp-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle for integrating with the Google DFP Ad Network",
    "keywords": ["ads", "dfp"],
    "homepage": "https://github.com/dbtlr/NodrewDfpBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Drew Butler",
            "homepage": "https://github.com/dbtlr"
        }
    ],

    "require": {
        "php":                      ">=5.3.2"
    },

    "require-dev": {
        "twig/twig":                ">=1.0,<2.0-dev",
        "symfony/yaml":             ">=2.0.16,~2.0"
    },

    "autoload": {
        "psr-0": { "Nodrew\\Bundle\\DfpBundle": "" }
    },

    "target-dir": "Nodrew/Bundle/DfpBundle"
}
